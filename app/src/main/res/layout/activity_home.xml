<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeActivity">

    <Button
        android:id="@+id/gameOfLifeConsoleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GameOfLive : Console"
        app:layout_constraintBottom_toTopOf="@id/gameOfLifeCustomViewButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_chainStyle="packed"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/gameOfLifeCustomViewButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GameOfLive : CustomView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/gameOfLifeConsoleButton" />

</androidx.constraintlayout.widget.ConstraintLayout>